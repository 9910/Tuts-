extends layout2

block content
  h2 Dashboard
  br
  br
  table.table.table-striped
    tr
      th ID
      th Project Name
      th Servise
      th
      th
    each row, i in rows
      tr
        td #{row.id}
        td #{row.title}
        td #{row.servise}
        td
          a.btn.btn-primary(href="/admin/edit/#{row.id}") Edit
        td
          a.btn.btn-danger(href="#" class="deleteProject" data-id="#{row.id}") Delete
  script(src="vendor/jquery/jquery.min.js")
  script(src="javascripts/dashboard.js")
