{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "underscore"
  ],
  "loadEagerly": [
    "path/to/your/js/**/*.js"
  ],
  "dontLoad": [
    "node_modules/**",
    "path/to/your/js/**/*.js"
  ],
  "plugins": {
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "es_modules": {},
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }
  }
}